CREATE TABLE Departments(
DeptID INT PRIMARY KEY,
DeptName VARCHAR(50)
);

CREATE TABLE Employees(
EmpID INT PRIMARY KEY,
EmpName VARCHAR(50),
DeptID INT NULL
);

INSERT INTO Departments VALUES
(1, 'Engineering'),
(2, 'HR'),
(3, 'Finance');

INSERT INTO Employees VALUES
(101, 'Amit', 1),
(102, 'Neha', 2),
(103, 'Rahul', NULL),
(104, 'Sonal', 99);

SELECT
e.EmpName,
d.DeptName
FROM Employees e
INNER JOIN Departments d
ON e.DeptID = d.DeptID;

SELECT
e.EmpName,
d.DeptName
FROM Employees e
LEFT JOIN Departments d
ON e.DeptID = d.DeptID;

SELECT
e.EmpName,
d.DeptName
FROM Employees e
RIGHT JOIN Departments d
ON e.DeptID = d.DeptID;
